{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/program.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "preLaunchTask": "gcc build active file",
            "sourceFileMap": {
                "${workspaceFolder}/dict.c": "${workspaceFolder}/dict.c",
                "${workspaceFolder}/graph.c": "${workspaceFolder}/graph.c",
                "${workspaceFolder}/graph_io.c": "${workspaceFolder}/graph_io.c",
                "${workspaceFolder}/main.c": "${workspaceFolder}/main.c",
                "${workspaceFolder}/names.c": "${workspaceFolder}/names.c",
                "${workspaceFolder}/stack.c": "${workspaceFolder}/stack.c",
                "${workspaceFolder}/vertex.c": "${workspaceFolder}/vertex.c",
            }
        }
    ]
}
